local allowedPlaceIds = {
    15346670220,
    16022223580,
    15558626179,
    16554222704,
    15002061926,
    15572953384,
    15191600573,
    15345591047,
    15566552620,
    16077173181,
    15410160852
}

local currentPlaceId = game.PlaceId
local isAllowedPlace = false

for _, placeId in ipairs(allowedPlaceIds) do
    if currentPlaceId == placeId then
        isAllowedPlace = true
        break
    end
end

if not isAllowedPlace then
    return
end

local alreadyPart = game.Workspace:FindFirstChild("Already")

if alreadyPart then
    print("Already running")
    return
else
    -- Если объекта нет, создаем его
    alreadyPart = Instance.new("Part")
    alreadyPart.Name = "Already"
    alreadyPart.Size = Vector3.new(1, 1, 1)
    alreadyPart.Anchored = true
    alreadyPart.Parent = game.Workspace
end

function GetPlayerInfo(player)
    return player.Name, player.UserId
end

function SendMessage(url, message)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    local data = {
        ["content"] = message
    }
    local body = http:JSONEncode(data)
    local response = request({
        Url = url,
        Method = "POST",
        Headers = headers,
        Body = body
    })
    print("Sent")
end

function SendMessageEMBED(url, embed)
    local http = game:GetService("HttpService")
    local headers = {
        ["Content-Type"] = "application/json"
    }
    local data = {
        ["embeds"] = {
            {
                ["title"] = embed.title,
                ["description"] = embed.description,
                ["color"] = embed.color,
                ["fields"] = embed.fields,
                ["footer"] = {
                    ["text"] = embed.footer.text
                }
            }
        }
    }
    local body = http:JSONEncode(data)
    local response = request({
        Url = url,
        Method = "POST",
        Headers = headers,
        Body = body
    })
    print("Sent")
end

local player = game.Players.LocalPlayer
local username, playerId = GetPlayerInfo(player)

local url = "https://discord.com/api/webhooks/1355434881970340003/4N8BCHOxgIDgjB2MKjOnFnzAfcTnlvi1Bw18gotRipcBGSIHLUzE02RcNd7wajEGaYSJ"
SendMessage(url, "")

local embed = {
    ["title"] = "Protection of DeathBall",
    ["description"] = "Exploit Detected",
    ["color"] = 1000,
    ["fields"] = {
        {
            ["name"] = username,
            ["value"] = "https://www.roblox.com/users/" .. playerId .. "/profile"
        },
    },
    ["footer"] = {
        ["text"] = playerId
    }
}
SendMessageEMBED(url, embed)

function getNil(name, class)
    for _, v in pairs(getnilinstances()) do
        if v.ClassName == class and v.Name == name then
            return v
        end
    end
end

local args = {
    [1] = nil
}

game:GetService("ReplicatedStorage").Actions.Action:FireServer(unpack(args))
